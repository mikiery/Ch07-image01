<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Silver"
    tools:context=".M0707">

    <tw.tcnr05.m0707.CircleImgView
        android:id="@+id/circleImgView"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="94dp"
        android:layout_marginBottom="377dp"
        android:scaleType="matrix"
        app:srcCompat="@drawable/img01th" />

    <TextView
        android:id="@+id/m0707_t001"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="74dp"
        android:layout_marginTop="415dp"
        android:text="@string/m0707_t001"
        android:textColor="@color/Navy"
        android:textSize="@dimen/fontsize" />

    <TextView
        android:id="@+id/myname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="41dp"
        android:text="@string/myname"
        android:textColor="@color/Navy"
        android:textSize="@dimen/fontsize" />

    <tw.tcnr05.m0707.CircleImgbutton
        android:id="@+id/circleImgbutton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="104dp"
        android:layout_marginTop="185dp"
        android:backgroundTint="#00000000"
        android:scaleType="matrix"
        app:srcCompat="@drawable/img02th" />

</RelativeLayout>